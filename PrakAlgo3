print('@@@  @@@@ @@@    @@    @@@ @@@@')
print('@  @ @  @  @@@   @@   @@@  @  @')
print('@@@@ @  @   @@@ @  @ @@@   @  @')
print('@  @ @  @    @@@    @@@    @  @')
print('@@@  @@@@     @@    @@     @@@@')
angka_awal = int(input("Masukkan angka awal: "))
angka_akhir = int(input("Masukkan angka akhir: "))

print(f"Urutan antara {angka_awal} dan {angka_akhir} adalah:")
while angka_awal <= angka_akhir:
    print(f"{angka_awal} | {angka_akhir}")
    angka_awal += 1
    angka_akhir -= 1
